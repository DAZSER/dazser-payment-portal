{
  "name": "dazser-payment-portal",
  "version": "4.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "nyc mocha",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:js": "eslint \"typescript/**/*.ts\"",
    "lint:css": "stylelint --config .stylelintrc --syntax scss \"scss/**/*.scss\"",
    "clean": "rm -rf .build/** public/js/** public/css/** public/images/** views/*.hbs views/**/*.hbs dist/**",
    "build": "npm-run-all --sequential clean --parallel img css js",
    "js": "npm-run-all --parallel js:frontend js:backend",
    "js:backend": "tsc -p tsconfig.backend.json",
    "js:frontend": "npm-run-all --parallel frontend:compile frontend:stripe --sequential frontend:inline",
    "frontend:compile": "browserify \"typescript/frontend/app.ts\" -p [ tsify -p tsconfig.frontend.json ] > public/js/bundle.js",
    "frontend:stripe": "tsc -p tsconfig.stripe.json",
    "frontend:inline": "inline-source --compress false html/portal.html views/portal.hbs && cp html/map.html views/map.hbs && cp html/old.html views/old.hbs && cp html/layouts/main.html views/layouts/main.hbs",
    "img": "cp images/jklogo-clear-SM2-fs8.png public/images/jklogo.png",
    "css": "npm-run-all --parallel css:compile --sequential css:prefix css:minify",
    "css:compile": "node-sass --output-style expanded --precision 6 scss/main.scss public/css/main.css",
    "css:prefix": "postcss --config postcss.config.js --replace \"public/css/*.css\" \"!public/css/*.min.css\"",
    "css:minify": "cleancss --level 1 --output public/css/main.min.css public/css/main.css",
    "deploy:dev": "sls deploy --stage dev -v",
    "deploy:prod": "sls deploy --stage prod -v",
    "rollback:prod": "sls rollback --timestamp 1557324780999 --stage prod -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/big.js": "^4.0.5",
    "@types/express": "^4.17.3",
    "@types/express-handlebars": "^3.1.0",
    "@types/inputmask": "^4.0.1",
    "@types/mysql": "^2.15.9",
    "@types/node": "^13.9.3",
    "@types/serve-favicon": "^2.5.0",
    "@stripe/stripe-js": "^1.2.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "autoprefixer": "^9.7.5",
    "aws-sdk": "^2.645.0",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "clean-css-cli": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "inline-source-cli": "^2.0.0",
    "mocha": "^7.1.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2",
    "serverless-domain-manager": "^3.3.1",
    "serverless-offline": "^6.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "big.js": "^5.2.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "formdata-polyfill": "^3.0.19",
    "inputmask": "^5.0.3",
    "mysql": "^2.18.1",
    "serve-favicon": "^2.5.0",
    "serverless-http": "^2.3.2",
    "stripe": "^8.33.0",
    "validator": "^13.0.0"
  }
}
