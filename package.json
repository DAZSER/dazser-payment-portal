{
  "name": "dazser-payment-portal",
  "version": "2.0.0",
  "description": "",
  "main": "index.html",
  "//": [
    "js-compile: This compiles the typescript, then it removes the module import from the ux.js file. (I'm statically calling a <script src> tag in html",
    "All my PRODUCTION dependencies are in the dependencies, even though I technically don't need formdata, inputmask, and weakmap node modules in production (see build.js)",
    "deploy-prod sls deploy --stage prod --customDomainEnabled true -v"
  ],
  "scripts": {
    "test": "nyc mocha",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:js": "tslint -c tslint.json 'typescript/*.ts'",
    "lint:css": "stylelint --config .stylelintrc --syntax scss \"scss/**/*.scss\"",
    "clean": "rm -rf .build/** public/js/** public/css/** public/images/** views/** backend.js email.js fee.js frontend.js logger.js",
    "compile": "npm-run-all --sequential clean --parallel img css js",
    "js": "npm-run-all --parallel js:frontend js:backend",
    "js:backend": "tsc -p tsconfig.backend.json",
    "js:frontend": "npm-run-all --sequential frontend:compile frontend:browserify frontend:inline",
    "frontend:compile": "tsc -p tsconfig.frontend.json && sed -i '/Object.defineProperty(exports, \"__esModule\", { value: true });/d' .build/js/stripe.js",
    "frontend:browserify": "browserify .build/js/app.js -o public/js/bundle.js",
    "frontend:inline": "inline-source --compress false html/index.html views/index.hbs && cp html/map.html views/map.hbs && cp html/old.html views/old.hbs",
    "img": "cp images/jklogo-clear-SM2-fs8.png public/images/jklogo.png",
    "css": "npm-run-all --parallel css:compile --sequential css:prefix css:minify",
    "css:compile": "node-sass --output-style expanded --precision 6 scss/main.scss public/css/main.css",
    "css:prefix": "postcss --config postcss.config.js --replace \"public/css/*.css\" \"!public/css/*.min.css\"",
    "css:minify": "cleancss --level 1 --output public/css/main.min.css public/css/main.css",
    "deploy:dev": "sls deploy --stage dev -v",
    "deploy:prod": "sls deploy --stage prod -v",
    "rollback:prod": "sls rollback --timestamp 1548711537153 --stage prod -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.20",
    "@types/big.js": "^4.0.5",
    "@types/express": "^4.16.1",
    "@types/express-handlebars": "0.0.30",
    "@types/inputmask": "^4.0.0",
    "@types/mysql": "^2.15.5",
    "@types/node": "^12.0.0",
    "@types/serve-favicon": "^2.2.30",
    "@types/stripe": "^6.19.8",
    "@types/stripejs": "^3.0.4",
    "@types/validator": "^10",
    "autoprefixer": "^9.4.9",
    "aws-sdk": "^2.413.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "clean-css-cli": "^4.2.1",
    "inline-source-cli": "^1.2.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "postcss-cli": "^6.1.2",
    "serverless-domain-manager": "^3.1.0",
    "serverless-offline": "^4.7.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "big.js": "^5.2.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.2",
    "formdata-polyfill": "^3.0.17",
    "inputmask": "^4.0.6",
    "mysql": "^2.16.0",
    "serve-favicon": "^2.5.0",
    "serverless-http": "^1.9.1",
    "stripe": "^6.25.1",
    "validator": "^10.11.0"
  }
}
