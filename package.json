{
  "name": "dazser-payment-portal",
  "version": "2.0.1",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "nyc mocha",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:js": "tslint -c tslint.json 'typescript/*.ts'",
    "lint:css": "stylelint --config .stylelintrc --syntax scss \"scss/**/*.scss\"",
    "clean": "rm -rf .build/** public/js/** public/css/** public/images/** views/*.hbs views/**/*.hbs backend.js email.js fee.js frontend.js logger.js",
    "build": "npm-run-all --sequential clean --parallel img css js",
    "js": "npm-run-all --parallel js:frontend js:backend",
    "js:backend": "tsc -p tsconfig.backend.json",
    "js:frontend": "npm-run-all --sequential frontend:compile frontend:browserify frontend:inline",
    "frontend:compile": "tsc -p tsconfig.frontend.json && sed -i '/Object.defineProperty(exports, \"__esModule\", { value: true });/d' .build/js/stripe.js",
    "frontend:browserify": "browserify .build/js/app.js -o public/js/bundle.js",
    "frontend:inline": "inline-source --compress false html/portal.html views/portal.hbs && cp html/map.html views/map.hbs && cp html/old.html views/old.hbs && cp html/layouts/main.html views/layouts/main.hbs",
    "img": "cp images/jklogo-clear-SM2-fs8.png public/images/jklogo.png",
    "css": "npm-run-all --parallel css:compile --sequential css:prefix css:minify",
    "css:compile": "node-sass --output-style expanded --precision 6 scss/main.scss public/css/main.css",
    "css:prefix": "postcss --config postcss.config.js --replace \"public/css/*.css\" \"!public/css/*.min.css\"",
    "css:minify": "cleancss --level 1 --output public/css/main.min.css public/css/main.css",
    "deploy:dev": "sls deploy --stage dev -v",
    "deploy:prod": "sls deploy --stage prod -v",
    "rollback:prod": "sls rollback --timestamp 1557324780999 --stage prod -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.26",
    "@types/big.js": "^4.0.5",
    "@types/express": "^4.16.1",
    "@types/express-handlebars": "0.0.32",
    "@types/inputmask": "^4.0.0",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.4",
    "@types/serve-favicon": "^2.2.30",
    "@types/stripe": "^6.26.5",
    "@types/stripejs": "^3.0.5",
    "@types/validator": "^10.11.0",
    "autoprefixer": "^9.6.0",
    "aws-sdk": "^2.467.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "clean-css-cli": "^4.3.0",
    "inline-source-cli": "^1.2.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "postcss-cli": "^6.1.2",
    "serverless-domain-manager": "^3.2.2",
    "serverless-offline": "^5.0.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "big.js": "^5.2.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "formdata-polyfill": "^3.0.18",
    "inputmask": "^4.0.8",
    "mysql": "^2.17.1",
    "serve-favicon": "^2.5.0",
    "serverless-http": "^2.0.2",
    "stripe": "^7.1.0",
    "validator": "^11.0.0"
  }
}
